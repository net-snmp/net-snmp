        { printf "%s\n" "$as_me:${as_lineno-$LINENO}: WARNING: Can't find system kernel... hopefully this won't be needed!" >&5
printf "%s\n" "$as_me: WARNING: Can't find system kernel... hopefully this won't be needed!" >&2;}
    fi

