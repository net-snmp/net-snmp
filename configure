_ACEOF

  else
    cat >>confdefs.h <<_ACEOF
#define NETSNMP_DEFAULT_SNMP_VERSION $ac_cv_user_prompt_NETSNMP_DEFAULT_SNMP_VERSION
_ACEOF

  fi
fi


