fi
rm -f core conftest.err conftest.$ac_objext \
    conftest$ac_exeext
  if ${ac_cv_search_dmalloc_message+:} false; then :
  break
fi
done
if ${ac_cv_search_dmalloc_message+:} false; then :

else
