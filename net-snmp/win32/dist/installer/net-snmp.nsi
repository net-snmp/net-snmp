      File "bin\snmpd.exe"

    SEC02_OpenSSL:
      File "bin.ssl\snmpd.exe"

    SetOutPath "$INSTDIR\share\snmp\snmpconf-data\snmpd-data"
    File "share\snmp\snmpconf-data\snmpd-data\*.*"
  
