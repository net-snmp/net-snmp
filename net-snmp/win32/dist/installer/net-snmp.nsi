    Delete "$INSTDIR\bin\snmpd.exe"  

    File "bin\msvcm90.dll"
    File "bin\msvcp90.dll"
    File "bin\msvcr90.dll"
    File "bin\Microsoft.VC90.CRT.manifest"

    StrCmp $openSSL "false" SEC02_noOpenSSL SEC02_OpenSSL
